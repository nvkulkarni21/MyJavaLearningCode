 */
public class Fibonacci {
    public static void main(String[] args){
        int ret = fibonacci(5);
        System.out.println("Value : " + ret);
    }

    public static int fibonacci(int n){
        if (n == 1)
            return 1;
        else if (n == 2)
            return 1;
        else {
            return fibonacci(n-1) + fibonacci(n-2);
        }
    }
}


